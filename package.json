{
  "name": "socket-io-tester",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "@types/jest": "24.0.19",
    "@types/node": "12.11.7",
    "@types/react": "16.9.11",
    "@types/react-dom": "16.9.3",
    "@types/react-redux": "^7.1.5",
    "husky": "^3.0.9",
    "jest": "^24.9.0",
    "lint-staged": "^9.4.2",
    "node-sass": "^4.13.1",
    "prettier": "^1.18.2",
    "prettier-stylelint": "^0.4.2",
    "react-scripts": "3.2.0",
    "redux-devtools-extension": "^2.13.8",
    "rimraf": "^3.0.0",
    "stylelint": "^11.1.1",
    "stylelint-config-standard": "^19.0.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.1.0",
    "stylelint-no-unsupported-browser-features": "^3.0.2",
    "stylelint-order": "^3.1.1",
    "stylelint-scss": "^3.12.0",
    "ts-jest": "^24.1.0",
    "tslint": "^5.20.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.0.1",
    "tslint-react": "^4.1.0",
    "typescript": "3.6.4"
  },
  "dependencies": {
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-redux": "^7.1.1",
    "redux": "^4.0.4",
    "redux-thunk": "^2.3.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "clean-node": "rimraf node_modules",
    "clean": "rimraf npm-debug.log* yarn-error.log*",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.tsx\"",
    "lint:ts": "tslint-config-prettier-check ./tslint.json",
    "lint:ts:fix": "yarn format || yarn lint:ts",
    "lint:scss": "stylelint 'src/**/*.scss' --syntax scss",
    "lint:scss:fix": "yarn lint:scss --fix",
    "lint": "yarn lint:ts:fix && yarn lint:scss:fix",
    "prebuild": "yarn test && yarn lint"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "yarn lint:ts:fix", "git add"
    ],
    "src/**/*.scss": [
      "yarn lint:scss:fix", "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
